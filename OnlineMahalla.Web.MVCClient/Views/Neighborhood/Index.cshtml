@using Microsoft.AspNetCore.Mvc.Localization
@inject IHtmlLocalizer<SharedResource> SharedLocalizer
@{
    ViewData["Title"] = "Index";
}
<environment names="Development">
    <link rel="stylesheet" href="~/lib/bootstrap-table/dist/bootstrap-table.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-datepicker/dist/css/bootstrap-datepicker.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/dist/css/bootstrap-select.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</environment>
<environment names="Staging,Production">
    <link rel="stylesheet" href="~/lib/bootstrap-table/dist/bootstrap-table.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/dist/css/bootstrap-select.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</environment>

<style>
    .bootstrap-table .table:not(.table-condensed), .bootstrap-table .table:not(.table-condensed) > tbody > tr > th, .bootstrap-table .table:not(.table-condensed) > tfoot > tr > th, .bootstrap-table .table:not(.table-condensed) > thead > tr > td, .bootstrap-table .table:not(.table-condensed) > tbody > tr > td, .bootstrap-table .table:not(.table-condensed) > tfoot > tr > td {
        padding: 8px;
        white-space: pre-wrap;
    }
</style>    

<h3 style="text-align:center">@SharedLocalizer["Organization"]</h3>
<div class="panel panel-primary"></div>
<div id="custom-toolbar" class="bs-bars pull-center">
    <div class="form-group form-inline" align="center">
        <div class="input-group">
            <input id="filterid" style="font-size:13px;width:100px" class="form-control input-sm" type="number" placeholder="@SharedLocalizer["ID"]" />
        </div>
        <div class="input-group">
            <input id="filterinn" style="font-size:13px;width:100px" maxlength="9" class="form-control input-sm" placeholder="@SharedLocalizer["INN"]" />
        </div>
        <div class="input-group">
            <input id="filtername" style="font-size:13px;width:300px" class="form-control input-sm" placeholder="@SharedLocalizer["Name"]" />
        </div>
        <div class="input-group">
            <input id="filterRegion" style="font-size:13px;width:200px" class="form-control input-sm" placeholder="@SharedLocalizer["Oblast"]" />
        </div>
        <div class="input-group">
            <input id="filterregion" style="font-size:13px;width:200px" class="form-control input-sm" placeholder="@SharedLocalizer["Region"]" />
        </div>
        <div class="input-group">
            <div class="btn-toolbar">
                <button id="refreshdata" class="btn btn-success btn-sm" data-toggle="popover" data-content="@SharedLocalizer["Refresh"]" data-trigger="hover"><span class="glyphicon glyphicon-refresh"></span></button>
                <button id="addRow" class="btn btn-primary btn-sm" data-toggle="popover" data-trigger="hover" data-content="@SharedLocalizer["Create"]"><span class="glyphicon glyphicon-plus"></span></button>
                <button id="cloneRow" type="button" data-toggle="popover" class="btn btn-primary btn-sm" data-trigger="hover" data-content="@SharedLocalizer["Clone"]"><span class="glyphicon glyphicon-duplicate"></span></button>
                <button id="editRow" class="btn btn-primary btn-sm" data-toggle="popover" data-trigger="hover" data-content="@SharedLocalizer["Edit"]"><span class="glyphicon glyphicon-edit"></span></button>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <table id="table"
           data-toolbar="#toolbar"
           data-search="false"
           data-show-refresh="false"
           data-show-toggle="false"
           data-show-columns="false"
           data-show-export="true"
           data-detail-view="false"
           data-minimum-count-columns="2"
           data-show-pagination-switch="false"
           data-pagination="true"
           data-id-field="ID"
           data-page-list="[50,100,150,200]"
           data-show-footer="false"
           data-side-pagination="server"
           data-url="@Url.Action("GetList", "Neighborhood")"
           data-query-params="queryParams"
           class="table table-striped">
        <thead>
            <tr>
                <th data-field="ID" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["ID"]</th>
                <th data-field="Name" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["Name"]</th>
                <th data-field="ChairmanName" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["ChairmanName"]</th>
                <th data-field="INN" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["INN"]</th>
                <th data-field="RegionName" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["RegionName"]</th>
                <th data-field="DistrictName" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["DistrictName"]</th>
                <th data-field="Address" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["DistrictName"]</th>
                <th data-field="PhoneNumber" data-align="left" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["PhoneNumber"]</th>
                <th data-field="State" data-align="center" data-valign="middle" data-sortable="true" data-cell-style="cellStyle">@SharedLocalizer["State"]</th>
            </tr>
        </thead>
    </table>
</div>
<div id="editWindow" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg" style="width:95%">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-body">
                <br />
                <div class="container-fluid">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h2 class="panel-title" style="text-align:center"><b>@SharedLocalizer["Organization"]</b></h2>
                        </div>
                        <div align="center">
                            <div class="form-horizontal">
                                <br />
                                <div class="form-group" align="center">
                                    <div class="form-group">
                                        <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["IncomeNumber"]</label>
                                        <div class="col-sm-1">
                                            <input placeholder="@SharedLocalizer["Number"]" style="font-size:15px" data-bind="value:organization.IncomeNumber" class="text-center form-control input-sm" data-width="100%" />
                                        </div>
                                        <label class="col-md-1 control-label" required style="font-size:12px;text-align:right">@SharedLocalizer["Date"]</label>
                                        <div class="col-md-1">
                                            <input placeholder="@SharedLocalizer["Date"]" data-bind="value:organization.IncomeDate,valueUpdate:'afterkeydown'" required class="text-center form-control input-sm" data-provide="datepicker" data-date-format="dd.mm.yyyy" data-date-autoclose="true" data-date-language="ru" data-width="100%" />
                                        </div>
                                        <form class="form-inline">
                                            <label class="col-md-1 control-label" style="font-size:12px;text-align:right">@SharedLocalizer["INN"]</label>
                                            <div class="col-sm-2">
                                                <input class="form-control input-sm" data-bind="value:organization.INN,valueUpdate:'afterkeydown'" type="number" style="width:56%">
                                                <button type="button" id="btncheckinn" class="btn btn-primary btn-sm"><span class="glyphicon glyphicon-check"></span></button>
                                            </div>
                                        </form>
                                        <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["Chapter"]</label>
                                        <div class="col-sm-2">
                                            <select style="font-size:12px" data-bind='options: chapterlist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.ChapterID,selectPicker: {}' data-live-search="true" class="" data-width="100%" data-size="15"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["Name"]</label>
                                        <div class="col-sm-9">
                                            <input placeholder="@SharedLocalizer["Name"]" style="font-size:15px" data-bind="value:organization.Name, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["FullName"]</label>
                                        <div class="col-sm-6">
                                            <input placeholder="@SharedLocalizer["FullName"]" style="font-size:15px" data-bind="value:organization.FullName, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                        </div>
                                        <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["OKED"]</label>
                                        <div class="col-sm-2">
                                            <select style="font-size:12px" data-bind='options: okedlist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.OKONHID,selectPicker: {}' data-live-search="true" class="" data-width="100%" data-size="15"></select>
                                        </div>
                                    </div>
                                    <div class="modal-body">

                                        <div class="table">
                                            <!-- Only required for left/right tabs -->
                                            <ul class="nav nav-tabs">
                                                <li class="active"><a href="#tab0" data-toggle="tab">@SharedLocalizer["OrganizationInfo"]</a></li>
                                                <li><a href="#tab1" data-toggle="tab">@SharedLocalizer["TrOrganizationAccount"]</a></li>
                                                <li><a href="#tab2" data-toggle="tab">@SharedLocalizer["OrganizationsFunctionalItem"]</a></li>
                                                <li><a href="#tab3" data-toggle="tab">@SharedLocalizer["OrganizationSign"]</a></li>
                                                <li><a href="#tab4" data-toggle="tab">@SharedLocalizer["OrganizationsDocSettAccount"]</a></li>
                                            </ul>
                                            <div class="tab-content">
                                                <div class="tab-pane active" id="tab0">
                                                    <br>
                                                    <div class="form-group">
                                                        <div class="form-group">
                                                            <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["Oblast"]</label>
                                                            <div class="col-sm-3">
                                                                <select data-bind='options: oblastlist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.OblastID,selectPicker: {},event:{ change: oblastchange}' data-live-search="true" class="" data-width="100%"> </select>
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["Region"]</label>
                                                            <div class="col-md-3">
                                                                <select data-bind='options: regionlist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.RegionID,selectPicker: {}' data-live-search="true" class="" data-width="100%"> </select>
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["ZipCode"]</label>
                                                            <div class="col-sm-1">
                                                                <input placeholder="@SharedLocalizer["ZipCode"]" style="font-size:15px" data-bind="value:organization.ZipCode" type="number" class="text-center form-control input-sm" data-width="100%" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["Adress"]</label>
                                                            <div class="col-sm-3">
                                                                <input placeholder="@SharedLocalizer["Adress"]" style="font-size:15px" data-bind="value:organization.Adress, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["ContactInfo"]</label>
                                                            <div class="col-sm-3">
                                                                <input placeholder="@SharedLocalizer["ContactInfo"]" style="font-size:15px" data-bind="value:organization.ContactInfo, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px"  >@SharedLocalizer["State"]</label>
                                            <div class="col-sm-1">
                                                <select style="font-size:15px"
                                                    data-bind='options: statelist, optionsText: "DisplayName",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.StateID,selectPicker: {}'
                                                    data-live-search="true" class="" data-width="95%"></select>
                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["Director"]</label>
                                                            <div class="col-sm-3">
                                                                <input placeholder="@SharedLocalizer["Director"]" style="font-size:15px" data-bind="value:organization.Director, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["Accounter"]</label>
                                                            <div class="col-sm-3">
                                                                <input placeholder="@SharedLocalizer["Accounter"]" style="font-size:15px" data-bind="value:organization.Accounter, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["Cashier"]</label>
                                                            <div class="col-sm-3">
                                                                <input placeholder="@SharedLocalizer["Cashier"]" style="font-size:15px" data-bind="value:organization.Cashier, event:{change: trimchange}" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["TreasuryDepartmentHeader"]</label>
                                                            <div class="col-sm-3">
                                                                <input style="font-size:15px" data-bind="value:organization.TreasuryDepartmentHeader" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["TreasuryResPerson"]</label>
                                                            <div class="col-sm-3">
                                                                <input style="font-size:15px" data-bind="value:organization.TreasuryResPerson" class="form-control input-sm" data-width="100%" />
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-2 control-label" style="font-size:12px">@SharedLocalizer["OrganizationType"]</label>
                                                            <div class="col-sm-3">
                                                                <select style="font-size:12px" data-bind='options: organizationtypelist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.OrganizationTypeID,selectPicker: {}' data-live-search="true" class="" data-width="100%"></select>
                                                            </div>
                                                            <label class="col-md-1 control-label" style="font-size:12px">@SharedLocalizer["HeaderOrganization"]</label>
                                                            <div class="col-sm-3">
                                                                <select style="font-size:12px" data-bind='options: headerorganizationlist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:organization.HeaderOrganizationID,selectPicker: {}' data-live-search="true" class="" data-width="100%"></select>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <form class="form-inline">
                                                                <label class="col-md-2 control-label" style="font-size:12px;text-align:right">@SharedLocalizer["CentralOrganization"]</label>
                                                                <div class="col-sm-4">
                                                                    <input class="form-control input-sm" data-bind="value:organization.CentralOrganizationName,valueUpdate:'afterkeydown'" style="width:86%">
                                                                    <button type="button" id="btnCentralOrganization" class="btn btn-primary btn-sm"><span class="glyphicon glyphicon-search"></span></button>
                                                                    <button type="button" id="btnselectreset"  class="btn btn-danger btn-sm"><span class="glyphicon glyphicon-remove"></span></button>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="tab-pane" style="overflow-x:auto; overflow-y:scroll; height:300px; z-index:auto" id="tab1">
                                                    <div class="panel panel-default">
                                                        <br>
                                                        <div class="form-group col-md-12" align="center">
                                                            <div>
                                                                <button align="center" type="button" data-bind='click: filldata' class="btn btn-primary btn-sm"><span>@SharedLocalizer["Fill"]</span></button>
                                                            </div>
                                                        </div>

                                                        <table class="table table-striped table-bordered table-condensed"
                                                               align="center" id='filterTableRole'>
                                                            <thead style="position:sticky; top:0; z-index: 2">
                                                                <tr>
                                                                    <th style="text-align:center">@SharedLocalizer["ID"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["Name"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["TrOrganizationAccount"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["Treasury"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["Bank"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["FunctionalItem"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["State"]</th>
                                                                    <th style="text-align:center">@SharedLocalizer["OutOfBalance"]</th>
                                                                    <th style="text-align:center;width:10%">@SharedLocalizer["OrgID"]</th>
                                                                    <th style="text-align:center;width:3%"><span class="label label-primary"></span></th>
                                                                </tr>
                                                                <tr>
                                                                    <td style="background-color:aquamarine" width="30px"></td>
                                                                    <td style="background-color:aquamarine" width="30px"></td>
                                                                    <td style="background-color:aquamarine" width="30px">
                                                                        <input class="form-control" type="text" id="settleInputRole" onkeyup="settleFilterRole()" placeholder="@SharedLocalizer["TrOrganizationAccount"]" title="@SharedLocalizer["TrOrganizationAccount"]">
                                                                    </td>
                                                                    <td style="background-color:aquamarine">
                                                                        <select data-bind='options: treasurybranchlist, optionsText: "Name",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:addtreasurybranchid,selectPicker:{}' data-live-search="true" class=""> </select>
                                                                    </td>
                                                                    <td style="background-color:aquamarine" width="30px">
                                                                        <select data-bind='options: banklist, optionsText: "Code",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:addbankid,selectPicker:{}' data-live-search="true" data-width="80px"> </select>
                                                                    </td>
                                                                    <td style="background-color:aquamarine" width="30px">
                                                                        <select data-bind='options: organizationfunctionalitemlist, optionsText: "Code",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:addorganizationfunctionalitemid1,selectPicker:{}' data-live-search="true" class="" data-width="100"> </select>

                                                                    </td>
                                                                    <td style="background-color:aquamarine" width="100px"></td>
                                                                    <td style="background-color:aquamarine" width="100px"></td>
                                                                    <td style="background-color:aquamarine" width="10px"></td>
                                                                    <td style="background-color:aquamarine" width="100px" align="center">
                                                                        <button type="button" data-bind='click: addRow1' class="btn btn-primary btn-xs"><span class="glyphicon glyphicon-plus"></span></button>
                                                                    </td>
                                                                </tr>
                                                            </thead>
                                                            <tbody data-bind="foreach:organization.SettlementAccount">
                                                                <!-- ko if: !(Status()===3) -->
                                                                <tr>
                                                                    <td style="width:5%">
                                                                        <span style="font-size:15px" data-bind='text:ID'></span>
                                                                    </td>
                                                                    <td>
                                                                        <!-- ko if: (Status()===1||Status()===2) -->
                                                                        <input data-bind="value:Name,valueUpdate:'afterkeydown'" class="form-control input-sm" type="text" />
                                                                        <!-- /ko -->
                                                                        <!-- ko if: (Status()===0) -->
                                                                        <span data-bind='text:Name'></span>
                                                                        <!-- /ko -->
                                                                    </td>
                                                                    <td style="width:18%">
                                                                        <!-- ko if: (Status()===1||Status()===2) -->
                                                                        <input data-bind="value:Code,valueUpdate:'afterkeydown'" class="form-control input-sm" type="text" />
                                                                        <!-- /ko -->
                                                                        <!-- ko if: (Status()===0) -->
                                                                        <span data-bind='text:Code'></span>
                                                                        <!-- /ko -->
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind='text:TreasuryBranchName'></span>
                                                                    </td>
                                                                    <td>
                                                                        <span data-bind='text:BankCode'></span>
                                                                    </td>
                                                                    <td>
                                                                        <!-- ko if: (Status()===1||Status()===0) -->
                                                                        <span data-bind='text:OrganizationFunctionalItemCode'></span>
                                                                        <!-- /ko -->

                                                                       @*  <!-- ko if: ((Status()===1||Status()===0) && OrganizationFunctionalItemCode() === null) -->
                                                                        <select data-bind='options: $root.organizationfunctionalitemlist, optionsText: "Code",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:OrganizationFunctionalItemID,selectPicker: {}' data-live-search="true" data-width="120px" class=""> </select>
                                                                        <!-- /ko -->*@
                                                                        <!-- ko if: (Status()===2) -->
                                                                        <select data-bind='options: $root.organizationfunctionalitemlist, optionsText: "Code",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:OrganizationFunctionalItemID,selectPicker: {}' data-live-search="true" data-width="120px" class=""> </select>
                                                                        <!-- /ko -->
                                                                    </td>
                                                                    <td>
                                                                        <!-- ko if: (Status()===1||Status()===2) -->
                                                                        <input data-bind="value:StateID,valueUpdate:'afterkeydown'" class="form-control input-sm" type="number" />

                                                                        @*<input data-bind="checked:IsActive,valueUpdate:'afterkeydown'" class="form-control input-sm" type="checkbox" />*@
                                                                        <!-- /ko -->
                                                                        <!-- ko if: (Status()===0) -->
                                                                        @*<input disabled data-bind='checked:IsActive' class="form-control input-sm" type="checkbox" />*@
                                                                        <input disabled data-bind="value:StateID,valueUpdate:'afterkeydown'" class="form-control input-sm" type="number" />

                                                                        <!-- /ko -->
                                                                    </td>
                                                                    <td>
                                                                        <!-- ko if: (Status()===1||Status()===2) -->
                                                                        <input data-bind="checked:OutOfBalance,valueUpdate:'afterkeydown'" class="form-control input-sm" type="checkbox" />
                                                                        <!-- /ko -->
                                                                        <!-- ko if: (Status()===0) -->
                                                                        <input disabled data-bind='checked:OutOfBalance' class="form-control input-sm" type="checkbox" />
                                                                        <!-- /ko -->
                                                                    </td>
                                                                    <td>
                                                                        <!-- ko if: (Status()===1||Status()===2) -->
                                                                        <input data-bind="value:OrganizationID,valueUpdate:'afterkeydown'" class="form-control input-sm" type="text" />
                                                                        <!-- /ko -->
                                                                        <!-- ko if: (Status()===0) -->
                                                                        <span data-bind='text:OrganizationID'></span>
                                                                        <!-- /ko -->
                                                                    </td>
                                                                    <td>
                                                                        <span type="button" data-bind='click: $root.editaccountRow' class="btn btn-default btn-xs" data-toggle="popover" data-trigger="hover"><i class="glyphicon glyphicon-edit"> </i></span>
                                                                        @*<span type="button" data-bind='click: $root.removeaccountRow' class="btn btn-danger btn-xs" data-toggle="popover" data-trigger="hover"><i class="glyphicon glyphicon-trash"> </i></span>*@
                                                                    </td>
                                                                </tr>
                                                                <!-- /ko -->
                                                            </tbody>

                                                        </table>
                                                    </div>
                                                </div>
                                                <div class="tab-pane" style="overflow-x:auto; overflow-y:scroll; height:300px; z-index:auto" id="tab2">
                                                    <br />
                                                    <hr />
                                                    <table class="table table-striped table-bordered table-condensed" align="center">
                                                        <thead>
                                                            <tr>
                                                                <th style="text-align:center">@SharedLocalizer["ID"]</th>
                                                                <th style="text-align:center">@SharedLocalizer["FunctionalItemCode"]</th>
                                                                <th style="text-align:center ;width:20%">@SharedLocalizer["FunctionalItemOfExpense"]</th>
                                                                <th style="text-align:center">@SharedLocalizer["Chapter"]</th>
                                                                <th style="text-align:center"><span class="label label-primary"></span></th>
                                                            </tr>
