            function initTable() {
                $table.bootstrapTable({
                    pageSize: 50,
                    height: getHeight()
                });
                $table.on('click-row.bs.table', function (e, row, $element) {
                    $('.success').removeClass('success');
                    $($element).addClass('success');
                    selectedrow = row.ID;
                    selectedid = row.ID;
                    userid = row.ID;
                });
                $table.on('load-error.bs.table', function (e, status, res) {
                    alert(res.responseText);
                });
                $tablemodule1.on('click-row.bs.table', function (e, row, $element) {
                    $('.success').removeClass('success');
                    $($element).addClass('success');
                    selectedrow = row.ID;
                });
                $tablemodule1.on('load-error.bs.table', function (e, status, res) {
                    alert(res.responseText);
                });
                $tablemodule2.on('click-row.bs.table', function (e, row, $element) {
                    $('.success').removeClass('success');
                    $($element).addClass('success');
                    selectedrow = row.ID;
                });
                $tablemodule2.on('load-error.bs.table', function (e, status, res) {
                    alert(res.responseText);
                });

                $tableorganization.on('click-row.bs.table', function (e, row, $element) {
                    $('.success').removeClass('success');
                    $($element).addClass('success');
                    selectedorgrow = row;
                });
                $tableorganization.on('load-error.bs.table', function (e, status, res) {
                    alert(res.responseText);
                });

                // sometimes footer render error.

                setTimeout(function () {
                    $table.bootstrapTable('resetView');
                }, 200);

                $(window).resize(function () {
                    $table.bootstrapTable('resetView', {
                        height: getHeight()
                    });
                });
            }
            function getHeight() {
                return $(window).height() - $('h3').outerHeight(true) * 2.5 - $('#custom-toolbar').outerHeight(true) * 2.5;
            }
            function editRow(id) {
                itemModel.loaddata(id);
                $("#editWindow").modal('show');
            }

            function deleteRow() {
                bootbox.confirm({
                    message: '@SharedLocalizer["YouWantToDelete"]',
                    buttons: {
                        cancel: {
                            label: '<i class="fa fa-times"></i> @SharedLocalizer["Cancel"]',
                            className: 'btn-danger'
                        },
                        confirm: {
                            label: '<i class="fa fa-check"></i> @SharedLocalizer["Confirm"]',
                            className: 'btn-success'
                        }
                    },
                    callback: function (result) {
                        if (result) {
                            $.ajax({
                                headers: {
                                    Accept: "application/json",
                                    "Content-Type": "application/json; charset=utf-8",
                                },
                                type: "DELETE",
                                url: "@Url.Action("Delete", "User")" + "/" + selectedrow
                            }).done(function (data, status, xhr) {
                                alert("@SharedLocalizer["SavedSuccessfully"]");
                                $table.bootstrapTable('refresh')
                            })
                                .fail(function (xhr, status, error) {
                                    alert("Error:" + error + " Message:" + xhr.responseText);
                                });
                        }
                    }
                });
            }
        });
