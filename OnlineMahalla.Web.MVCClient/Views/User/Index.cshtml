@using Microsoft.AspNetCore.Mvc.Localization
@inject IHtmlLocalizer<SharedResource> SharedLocalizer
@{
    ViewData["Title"] = "Index";
}

<environment names="Development">
    <link rel="stylesheet" href="~/lib/bootstrap-table/dist/bootstrap-table.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-datepicker/dist/css/bootstrap-datepicker.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/dist/css/bootstrap-select.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</environment>
<environment names="Staging,Production">
    <link rel="stylesheet" href="~/lib/bootstrap-table/dist/bootstrap-table.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/dist/css/bootstrap-select.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</environment>

<style type="text/css">
    .tg {
        border-collapse: collapse;
        border-spacing: 0;
    }

        .tg td {
            font-family: Arial, sans-serif;
            font-size: 14px;
            padding: 10px 5px;
            border-style: solid;
            border-width: 1px;
            overflow: hidden;
            word-break: normal;
            border-color: black;
        }

        .tg th {
            font-family: Arial, sans-serif;
            font-size: 14px;
            font-weight: normal;
            padding: 10px 5px;
            border-style: solid;
            border-width: 1px;
            overflow: hidden;
            word-break: normal;
            border-color: black;
            background-color: #FFFFFF;
        }

        .tg .tg-s268 {
            text-align: left
        }

    .bootstrap-table .table:not(.table-condensed),
    .bootstrap-table .table:not(.table-condensed) > tbody > tr > th,
    .bootstrap-table .table:not(.table-condensed) > tfoot > tr > th,
    .bootstrap-table .table:not(.table-condensed) > thead > tr > td,
    .bootstrap-table .table:not(.table-condensed) > tbody > tr > td,
    .bootstrap-table .table:not(.table-condensed) > tfoot > tr > td {
        padding: 8px;
        white-space: nowrap;
    }

    .exchange-btns {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        gap: 12px;
    }
</style>

<body>
    @Html.AntiForgeryToken()

    <h3 style="text-align:center">@SharedLocalizer["User"]</h3>
    <div class="panel panel-primary"></div>
    <div id="custom-toolbar" class="bs-bars pull-center">
        <div class="form-group form-inline" align="center">
            <div class="input-group" style="width:10px"></div>
            <div class="input-group" style="width:10px"></div>
            <button id="refreshdata" class="btn btn-success btn-sm" data-toggle="popover"
                    data-content="@SharedLocalizer["Refresh"]" data-trigger="hover">
                <i class="glyphicon glyphicon-refresh"></i>
            </button>
            <div class="input-group">
                <div class="btn-toolbar">
                    <div class="input-group" style="width:10px"></div>
                    <button type="button" class="btn btn-primary btn-sm" data-toggle="modal"
                            data-target="#exampleModal">
                        <span class="glyphicon glyphicon-filter"></span>
                    </button>
                    <button data-roleview="RoleEdit" id="addRow" class="btn btn-primary btn-sm" data-toggle="popover"
                            data-content="@SharedLocalizer["Create"]" data-trigger="hover">
                        <i class="fa fa-plus"></i>@*@SharedLocalizer["Edit"]*@
                    </button>
                    <div class="input-group" style="width:10px"></div>
                    <button data-roleview="RoleEdit" id="addRole" class="btn btn-primary btn-sm" data-toggle="popover"
                            data-content="@SharedLocalizer["Role"]" data-trigger="hover">
                        <i class="glyphicon glyphicon-pencil"></i>@*@SharedLocalizer["Edit"]*@
                    </button>
                    <div class="input-group" style="width:10px"></div>
                    <button data-roleview="UserEdit" id="editRow" class="btn btn-primary btn-sm" data-toggle="popover"
                            data-content="@SharedLocalizer["Edit"]" data-trigger="hover">
                        <i class="fa fa-edit"></i>@*@SharedLocalizer["Edit"]*@
                    </button>
                    <div class="input-group" style="width:10px"></div>
                    <button data-roleview="RoleDelete" id="deleteRow" type="button" class="btn btn-danger btn-sm"
                            data-toggle="popover" data-trigger="hover" data-content="@SharedLocalizer["Delete"]">
                        <i class="glyphicon glyphicon-trash"></i>@*@SharedLocalizer["Accept"]*@
                    </button>
                </div>
            </div>
        </div>
        <div class="modal fade bd-example-modal-lg" id="exampleModal" tabindex="-1" role="dialog"
             aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document" style="width:500px">
                <div class="modal-content">
                    <div style="background-color:#e0e0e0" class="modal-header">
                        <button id="clos" type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <b><span aria-hidden="true">&times;</span></b>
                        </button>
                        <h3 class="modal-title" style="text-align:center" id="exampleModalLabel"><b>Фильтр</b></h3>
                    </div>
                    <div style="background-color:#e0e0e0" class="form-horizontal">
                        <br>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["ID"]</label>
                            <b>
                                <input id="filterid" style="font-size:14px;width:150px"
                                       class="form-control input-sm col-md-2" type="number"
                                       placeholder="@SharedLocalizer["ID"]" />
                            </b>
                        </div>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["Name"]</label>
                            <b>
                                <input id="filtername" style="font-size:14px;width:150px"
                                       class="form-control input-sm col-md-2" placeholder="@SharedLocalizer["Name"]" />
                            </b>
                        </div>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["F.I.O."]</label>
                            <b>
                                <input id="filterfio" style="font-size:14px;width:150px"
                                       class="form-control input-sm col-md-2"
                                       placeholder="@SharedLocalizer["F.I.O."]" />
                            </b>
                        </div>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["State"]</label>
                            <b>
                                <input id="filterstate" style="font-size:14px;width:150px"
                                       class="form-control input-sm col-md-2"
                                       placeholder="@SharedLocalizer["State"]" />
                            </b>
                        </div>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["Organization"]</label>
                            <b>
                                <input id="filterorg" style="font-size:14px;width:237px"
                                       class="form-control input-sm col-md-2"
                                       placeholder="@SharedLocalizer["Organization"]" />
                            </b>
                        </div>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["INN"]</label>
                            <b>
                                <input id="filterinn" style="font-size:14px;width:237px"
                                       class="form-control input-sm col-md-2" placeholder="@SharedLocalizer["INN"]" />
                            </b>
                        </div>
                        <div class="form-group">
                            <label style="font-size:14px"
                                   class="col-md-4 col-xs-6 control-label">@SharedLocalizer["OrganizationID"]</label>
                            <b>
                                <input id="filterorgid" style="font-size:14px;width:150px"
                                       class="form-control input-sm col-md-2" type="number"
                                       placeholder="@SharedLocalizer["OrganizationID"]" />
                            </b>
                        </div>
                        <div style="text-align:center" class="modal-footer">
                            <button id="refreshdata2" class="btn-success btn-sm"
                                    type="button">
                                <span>@SharedLocalizer["ToApply"]</span>
                            </button>
                            <button id="resetdata" type="button" class="btn-primary btn-sm"
                                    style="background-color:#119eda">
                                <span>@SharedLocalizer["Reset"]</span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
<p></p>

<div class="container">
    <table id="table" data-toolbar="#toolbar" data-search="false" data-show-refresh="false" data-show-toggle="false"
           data-show-columns="false" data-show-export="true" data-detail-view="false" data-minimum-count-columns="2"
           data-show-pagination-switch="false" data-pagination="true" data-id-field="ID" data-page-list="[50,100,150,200]"
           data-show-footer="false" data-footer-style="footerStyle" data-side-pagination="server"
           data-url="@Url.Action("GetList", "User")" data-query-params="queryParams" class="table table-striped">
        <thead>
            <tr>
                <th data-field="ID" data-align="center" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["ID"]</th>
                <th data-field="Name" data-align="left" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["Name"]</th>
                <th data-field="DisplayName" data-align="left" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["F.I.O."]</th>
                <th data-field="State" data-align="center" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["State"]</th>
                <th data-field="VerifyEDS" data-align="center" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["VerifyEDS"]</th>
                <th data-field="INN" data-align="center" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["INN"]</th>
                <th data-field="Organization" data-align="left" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["Organization"]</th>
                <th data-field="OrganizationID" data-align="center" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["OrganizationID"]</th>
                <th data-field="LastAccessTime" data-align="left" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["LastAccessTime"]</th>
                <th data-field="LastIP" data-align="left" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["LastIP"]</th>
                <th data-field="Email" data-align="left" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["Email"]</th>
                <th data-field="DateOfModified" data-align="center" data-valign="middle" data-sortable="true"
                    data-cell-style="cellStyle">@SharedLocalizer["DateOfModified"]</th>
            </tr>
        </thead>
    </table>
</div>

<div id="editWindow" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg" style="width:1024px">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" style="text-align:center"><b>@SharedLocalizer["User"]</b></h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <div class="panel panel-default">
                        <div align="center">
                            <div class="form-horizontal">
                                <br />
                                <div class="form-group" align="center">
                                    <div class="form-group">
                                        <form class="form-inline">
                                            <label class="col-md-2 control-label" required
                                                   style="font-size:12px;text-align:right">@SharedLocalizer["Name"]</label>
                                            <div class="col-sm-4">
                                                <input placeholder="@SharedLocalizer["Name"]"
                                                       style="font-size:15px;width:82%" data-bind="value:user.Name"
                                                       required class="text-left form-control input-sm" />
                                                <button type="button" id="btnselectdatahistoryName"
                                                        class="btn btn-group btn-sm">
                                                    <span class="glyphicon glyphicon-option-horizontal"></span>
                                                </button>
                                            </div>
                                        </form>
                                        <form class="form-inline">
                                            <label class="col-md-1 control-label"
                                                   style="font-size:12px">@SharedLocalizer["F.I.O."]</label>
                                            <div class="col-sm-4">
                                                <input placeholder="@SharedLocalizer["F.I.O."]"
                                                       style="font-size:15px;width:82%" data-bind="value:user.DisplayName"
                                                       required class="text-left form-control input-sm" />
                                                <button type="button" id="btnselectdatahistoryDisplayName"
                                                        class="btn btn-group btn-sm">
                                                    <span class="glyphicon glyphicon-option-horizontal"></span>
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                    <div class="form-group">
                                        <form class="form-inline">
                                            <label class="col-md-2 control-label"
                                                   style="font-size:12px;text-align:right">@SharedLocalizer["OrganizationName"]</label>
                                            <div class="col-sm-4">
                                                <input disabled class="form-control input-sm"
                                                       data-bind="value:user.NeighborhoodName,valueUpdate:'afterkeydown'"
                                                       style="font-size:15px;width:82%">
                                                <button type="button" id="btnOrganization"
                                                        class="btn btn-primary btn-sm">
                                                    <span class="fa fa-search"></span>
                                                </button>
                                            </div>
                                        </form>
                                        <form class="form-inline">
                                            <label class="col-md-1 control-label"
                                                   style="font-size:12px;text-align:right">@SharedLocalizer["State"]</label>
                                            <div class="col-sm-4">
                                                <select style="font-size:15px"
                                                        data-bind='options: statelist, optionsText: "DisplayName",optionsValue:"ID", optionsCaption: "@SharedLocalizer["PleaseSelectOne"]", value:user.StateID,selectPicker: {}'
                                                        data-live-search="true" class="" data-width="95%"></select>
                                            </div>
                                        </form>
                                    </div>
                                    <div class="form-group">
                                        <form class="form-inline">
                                            <label class="col-md-2 control-label"
                                                   style="font-size:12px;text-align:right">@SharedLocalizer["Password"]</label>
                                            <div class="col-sm-4">
                                                <input placeholder="@SharedLocalizer["Password"]"
                                                       style="font-size:15px;width:95%" data-bind="value:user.Password"
                                                       class="text-left form-control input-sm" />
                                            </div>
                                        </form>
                                        <form class="form-inline">
                                            <label class="col-md-1 control-label"
                                                   style="font-size:12px">@SharedLocalizer["ConfirmedPassword"]</label>
                                            <div class="col-sm-4">
                                                <input placeholder="@SharedLocalizer["ConfirmedPassword"]"
                                                       style="font-size:15px;width:95%"
                                                       data-bind="value:user.PasswordConfirm"
                                                       class="text-left form-control input-sm" />
                                            </div>
                                        </form>
                                    </div>
                                    <div class="form-group">
                                        <form class="form-inline">
                                            <label class="col-md-2 control-label" required
                                                   style="font-size:12px;text-align:right">@SharedLocalizer["AllowedIP"]</label>
                                            <div class="col-sm-4">
                                                <input placeholder="@SharedLocalizer["AllowedIP"]"
                                                       style="font-size:15px;width:95%" data-bind="value:user.AllowedIP"
                                                       required class="text-left form-control input-sm" />
                                            </div>
                                        </form>
                                        <form class="form-inline">
                                            <label class="col-md-1 control-label" required
                                                   style="font-size:12px;text-align:right">@SharedLocalizer["VerifyEDS"]</label>
                                            <div class="col-sm-1">
                                                <input placeholder="@SharedLocalizer["VerifyEDS"]"
                                                       style="width: 30px;align-content:center"
                                                       data-bind="user.VerifyEDS,valueUpdate:'afterkeydown'"
                                                       type="checkbox" class="form-control input-sm" />
                                            </div>
                                        </form>
                                    </div>
                                    <br />
                                    <div class="form-group">
                                        <div style="align-content:center">
                                            <button type="button" data-bind="click:closewindow"
                                                    class="btn btn-default btn-sm">
                                                @SharedLocalizer["Close"]
                                            </button>
                                            <button type="button" data-bind="click:savedata"
                                                    class="btn btn-primary btn-sm">
                                                @SharedLocalizer["Save"]
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

